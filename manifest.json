{
    "description": "A Chromium theme with the colors of the DogeHouse app.",
    "author": "Alexander Johansen (Odyssey346)",
    "manifest_version": 2,
    "name": "DogeHouse Theme for Chromium Browsers",
    "version": "1.0",
    "theme": {
        "colors": {
            "bookmark_text": [ 255, 255, 255 ],
            "frame": [ 21, 26, 33 ],
            "nt_header": [ 255, 255, 255 ],
            "ntp_background": [ 11, 14, 17 ],
            "toolbar_button_icon": [ 253, 77, 77 ],
            "ntp_link": [ 0, 0, 0 ],
            "ntp_section": [ 35, 35, 35 ],
            "ntp_section_link": [ 0, 0, 0 ],
            "ntp_section_text": [ 0, 0, 0 ],
            "ntp_text": [ 255, 255, 255 ],
            "tab_background_text": [ 255, 255, 255 ],
            "tab_text": [ 255, 255, 255 ],
            "toolbar": [ 11, 14, 17 ],

            "omnibox_background":   [21, 26, 33]
        },
        "properties": {
            "ntp_background_alignment": "top center",
            "ntp_background_repeat": "no-repeat",
            "ntp_logo_alternate": 0
        },
        "tints": {
            "buttons": [ 255, 255, 255 ]
        }
    }
